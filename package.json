{
  "name": "mysys-api",
  "author": "brunocesarangst <bruno.angst@rede.ulbra.br>",
  "description": "App mySys - TypeScript, Nodejs, API",
  "repository": "https://github.com/mySyst/mysys-api.git",
  "license": "MIT",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "keywords": [
    "node.js",
    "api"
  ],
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --transpile-only src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "set NODE_ENV=development&& jest --projects ./test --forceExit --runInBand",
    "test:unit": "set NODE_ENV=development&& jest",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.10.4",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "jest": "^26.6.3",
    "nock": "^13.0.11",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.0.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.12.3"
  }
}
